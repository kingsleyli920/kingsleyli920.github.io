<% if (theme.socialLink.github) { %>
    <a href="<%= theme.socialLink.github %>" class="tooltipped" target="_blank" data-tooltip="Access to My GitHub" data-position="top" data-delay="50">
        <i class="fa fa-github"></i>
    </a>
<% } %>

<% if (theme.socialLink.email) { %>
    <a href="mailto:<%= theme.socialLink.email %>" class="tooltipped" target="_blank" data-tooltip="Email Me" data-position="top" data-delay="50">
        <i class="fa fa-envelope-open"></i>
    </a>
<% } %>

<% if (theme.socialLink.qq) { %>
    <a href="tencent://AddContact/?fromId=50&fromSubId=1&subcmd=all&uin=<%= theme.socialLink.qq %>" class="tooltipped" data-tooltip="QQ联系我: <%= theme.socialLink.qq %>" data-position="top" data-delay="50">
        <i class="fa fa-qq"></i>
    </a>
<% } %>

<% if (theme.socialLink.linkedin) { %>
    <a href="<%= theme.socialLink.linkedin %>" class="tooltipped" data-tooltip="My LinkedIn: <%= theme.socialLink.linkedin %>" data-position="top" data-delay="50">
        <i class="fa fa-linkedin"></i>
    </a>
<% } %>

<% if (theme.socialLink.wechat) { %>
    <a href="" class="tooltipped" data-tooltip="My WeChat: <%= theme.socialLink.wechat %>" data-position="top" data-delay="50">
        <i class="fa fa-weixin"></i>
    </a>
<% } %>


<% if (config.feed && config.feed.path == 'atom.xml') { %>
    <a href="<%- url_for('/atom.xml') %>" class="tooltipped" target="_blank" data-tooltip="RSS 订阅" data-position="top" data-delay="50">
        <i class="fa fa-rss"></i>
    </a>
<% } %>
